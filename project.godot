config_version=5

[application]

config/name="Nope! Not a Chance."
run/main_scene="res://scenes/main.tscn"
config/use_custom_user_dir=true
config/custom_user_dir_name="Godot/app_userdata/NopeNotAChance"
config/features=PackedStringArray("4.2", "Forward Plus")
boot_splash/bg_color=Color(0.0705882, 0.0705882, 0.0705882, 1)
boot_splash/show_image=false
boot_splash/fullsize=false
config/icon="res://icon.png"
config/windows_native_icon="res://icon.ico"

[autoload]

DataManager="*res://managers/data_manager.gd"
AnimationManager="*res://managers/animation_manager.gd"
AudioManager="*res://managers/audio_manager.tscn"
FontManager="*res://managers/font_manager.gd"
SceneManager="*res://managers/scene_manager.gd"
NotificationManager="*res://managers/notification_manager.gd"

[display]

window/size/viewport_width=1280
window/size/viewport_height=720
window/stretch/mode="canvas_items"

[file_customization]

folder_colors={
"res://audio/": "yellow",
"res://fonts/": "orange",
"res://images/": "blue",
"res://managers/": "green",
"res://scenes/": "green"
}

